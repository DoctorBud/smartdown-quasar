{
  "name": "smartdown-quasar",
  "version": "0.0.4",
  "description": "Smartdown Editor via Quasar/VueJS",
  "productName": "Smartdown Editor",
  "author": "Daniel B Keith",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "lint-fix": "eslint --fix --ext .js,.vue ./",
    "dev": "quasar dev",
    "build": "quasar build",
    "builds": "npm run build && http-server -c-1 dist/spa",
    "dev:electron": "quasar dev -m electron",
    "dev:android": "quasar dev -m android",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "build:electron": "quasar build -m electron",
    "build:ios": "quasar build -m capacitor -T ios",
    "build:ioside": "quasar build -m capacitor -T ios --ide",
    "publish": "./publish.sh",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@capacitor/core": "^3.6.0",
    "@capacitor/device": "^1.1.2",
    "@capacitor/dialog": "^1.0.7",
    "@capacitor/filesystem": "^1.1.0",
    "@capacitor/splash-screen": "^1.2.2",
    "@capacitor/status-bar": "^1.0.8",
    "@octokit/core": "^3.6.0",
    "@quasar/extras": "^1.14.2",
    "core-js": "^3.23.3",
    "electron": "19.0.7",
    "highlight.js": "^11.5.1",
    "prismjs": "^1.28.0",
    "prosemirror-example-setup": "^1.2.1",
    "prosemirror-highlightjs": "^0.8.0",
    "prosemirror-markdown": "^1.9.2",
    "prosemirror-model": "^1.18.1",
    "prosemirror-schema-basic": "^1.2.0",
    "prosemirror-schema-list": "^1.2.0",
    "prosemirror-state": "^1.4.1",
    "prosemirror-view": "^1.26.4",
    "quasar": "^2.7.5",
    "smartdown": "^1.0.53",
    "vue": "3",
    "vue-i18n": "^9.1.10",
    "vue-prism-editor": "^2.0.0-alpha.2",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@capacitor/cli": "^3.6.0",
    "@capacitor/ios": "^3.6.0",
    "@quasar/app-webpack": "^3.5.7",
    "electron-packager": "^15.5.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
