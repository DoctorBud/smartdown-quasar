{
  "name": "smartdown-quasar",
  "version": "0.0.10",
  "description": "Smartdown Editor via Quasar/VueJS",
  "productName": "Smartdown Editor",
  "author": "Daniel B Keith",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "lint-fix": "eslint --fix --ext .js,.vue ./",
    "dev": "quasar dev",
    "ldev": "SMARTDOWN_PREFIX=https://localhost:4011/lib GALLERY_DEV_MODE=1 npm run dev",
    "build": "quasar build",
    "builds": "npm run build && http-server -c-1 dist/spa",
    "dev:electron": "quasar dev -m electron",
    "dev:android": "quasar dev -m android",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "build:electron": "quasar build -m electron",
    "build:ios": "quasar build -m capacitor -T ios",
    "build:ioside": "quasar build -m capacitor -T ios --ide",
    "publish": "./publish.sh",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@capacitor/core": "^4.7.3",
    "@capacitor/device": "^4.1.0",
    "@capacitor/dialog": "^4.1.0",
    "@capacitor/filesystem": "^4.1.4",
    "@capacitor/splash-screen": "^4.2.0",
    "@capacitor/status-bar": "^4.1.1",
    "@octokit/core": "^4.2.1",
    "@quasar/extras": "^1.16.4",
    "core-js": "^3.30.2",
    "electron": "24.3.1",
    "highlight.js": "^11.8.0",
    "monaco-editor": "^0.38.0",
    "monaco-editor-vue3": "^0.1.6",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "prismjs": "^1.29.0",
    "prosemirror-example-setup": "^1.2.2",
    "prosemirror-highlightjs": "^0.9.1",
    "prosemirror-markdown": "^1.11.0",
    "prosemirror-model": "^1.19.1",
    "prosemirror-schema-basic": "^1.2.2",
    "prosemirror-schema-list": "^1.2.3",
    "prosemirror-state": "^1.4.3",
    "prosemirror-view": "^1.31.3",
    "quasar": "^2.12.0",
    "smartdown": "^1.0.59",
    "vue": "3",
    "vue-i18n": "^9.2.2",
    "vue-prism-editor": "^2.0.0-alpha.2",
    "vue-router": "4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.8",
    "@capacitor/cli": "^4.7.3",
    "@capacitor/ios": "^4.7.3",
    "@quasar/app-webpack": "^3.9.2",
    "electron-packager": "^17.1.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.13.0",
    "eslint-webpack-plugin": "^4.0.1"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
